#!/usr/bin/env python
import sys

def args() -> str:
	try:
		return open(sys.argv[1]).read().rstrip()
	except:
		return "Incorrect input format. Please input a file"

def main() -> None:
	dna = args()
	sequence = list(string)
	for letter in range(len(sequence)):
    		if sequence[letter] == 'T':
        	sequence[letter] = 'U'
        
	rna = "".join(sequence)
	print(rna)

if __name__ == '__main__':
	main()

